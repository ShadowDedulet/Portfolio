<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Comfortaa:wght@300;400;500&family=Montserrat:wght@100;200;300;400;500&display=swap');
    </style>

    <title>ShadowDedulet</title>
    <link rel="icon" href="shadowdedulet/img/logo2.png">
    <link rel="stylesheet" type="text/css" href="shadowdedulet/css/style.css">
    <link rel="stylesheet" type="text/css" href="shadowdedulet/css/aboutme.css">

    <script id="md5" src="https://cdnjs.cloudflare.com/ajax/libs/blueimp-md5/2.19.0/js/md5.min.js"></script>
    <script id="mainScript" type="module" src="shadowdedulet/js/index.js" defer></script>
</head>

<body>
    <%- include('partials/header.ejs', LOC_ELEMENTS, LOCALE, URL, PARAMS) %>

    <%- include('partials/messageTop.ejs', LOC_INFO) %>

    <%- include('partials/signForm.ejs', LOC_ELEMENTS, LOCALE, URL) %>

    <div id='content' class="content">
        <div class="aboutme">
            <% for (let line of LOC_TEXT.aboutMeHeader) { %>
            <p><%- line %> </p>
            <% } %>
            <ul>
                <li>C++, Python, JavaScript, TypeScript</li>
                <li>HTML, CSS, Markdown</li>
                <li>SQL, T-SQL</li>
                <li>Node.js, Express</li>
                <li>Mongodb, SQL Server Management Studio</li>
                <li>Git, Github, Travis CI, CircleCI</li>
                <li>Adobe Photoshop</li>
                <li>Windows 10, Arch Linux</li>
            </ul>
            <% for (let line of LOC_TEXT.aboutMeFooter) { %>
            <p><%- line %> </p>
            <% } %>
        </div>
    </div>

    <%- include('partials/footer.ejs', LOC_ELEMENTS, LOCALE, URL, PARAMS) %>

</body>