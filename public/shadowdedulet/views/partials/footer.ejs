<link rel="stylesheet" type="text/css" href="shadowdedulet/css/partials/footer.css">

<div class="footer">
    <div class="fcontainer">
        <a href="//<%= URL.host %>/about?language=<%= LOCALE %>"><%= LOC_ELEMENTS.aboutMe %></a>
    </div>

    <div class="fcontainer contacts">
        <div class="contact">
            <a href="https://github.com/shadowdedulet" target="blank">github</a>
        </div>
        <div class="contact">
            <p id='discordBtn'>discord</p>
            <p id='discordCopied'>copied</p>
        </div>
        <div class="contact">
            <a href="https://vk.com/ijustwannabealone" target="blank">vk</a>
        </div>
        <div class="contact">
            <a href="https://instagram.com/shadowdedulet" target="blank">instagram</a>
        </div>
    </div>

    <div class="fcontainer" id='languageSelector'>
        <p id='currentLanguage'><%= LOC_ELEMENTS.current %></p>
        <img src="shadowdedulet/img/languages/eng.png" alt="K" class="languageIcon">

        <div class="languageContainer" id='languageContainer'>
            <div class="languageName">
                <a href="//<%- URL.baseUrl %>?language=en"><%= LOC_ELEMENTS.eng %></a>
                <img src="shadowdedulet/img/languages/eng.png" alt="K" class="languageIcon">
            </div>

            <div class="languageName">
                <a href="//<%- URL.baseUrl %>?language=ru"><%= LOC_ELEMENTS.rus %></a>
                <img src="shadowdedulet/img/languages/rus.png" alt="K" class="languageIcon">
            </div>
        </div>
    </div>
</div>